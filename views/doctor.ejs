    <script type="text/ng-template" id="write-prescription-modal.html">
         <div class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
              <div class="title">Test Result </div>
                
                    <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true" style="color:red;font-size: 32px">&times;</button>
                </div>                                     
             
              
              <div class="modal-body">
                    <div ng-if="isToPrescribe">
                      <div style="max-height: 400px;overflow-y: auto;">
                      <ul class="nav">
                        <li ng-repeat="test in testObj.test_result"><strong>{{test.name}} :</strong> {{test.data}}</li>
                      </ul>
                      <p><strong>Conclusion :</strong> {{testObj.conclusion}}</p>
                      </div>
                      <div class="pl-0">
                        <label>Provisional diagnosis: </label>
                        <textarea  class="form-control p-1" placeholder="Write..." ng-model="patient.provisional_diagnosis"></textarea>
                      </div> 
                      <div>
                        <table class="table table-bordered table-responsive">
                          <tr>
                            <thead>
                              <th style="padding: 0"><strong>S/N</strong></th>
                              <th style="padding: 0"><strong>Drug name</strong></th>
                              <th style="padding: 0"><strong>Dosage</strong></th>
                              <th style="padding: 0"><strong>Frequency</strong></th>
                              <th style="padding: 0"><strong>Duration</strong></th>
                            </thead>
                          </tr>
                          <tr>
                            <tbody ng-repeat="prescription in drugList">                
                              <td style="padding: 0">{{prescription.sn}}</td>
                              <td style="padding: 0"><input name="states" id="states" type="text" placeholder="Enter drug name" ng-model="drug.name" uib-typeahead="drug.name for drug in drugs | filter:$viewValue | limitTo:15" class="form-control" style="width: 95%">
                                {{getDrug(drug.name)}} </td>
                              <td style="padding: 0"><input type="text" class="form-control" ng-model="prescription.dosage"></td>
                              <td style="padding: 0"><input type="text" class="form-control" ng-model="prescription.frequency"></td>
                              <td style="padding: 0"><input type="text" class="form-control" ng-model="prescription.duration"></td>
                            </tbody>
                          </tr>           
                        </table>
                      </div>
                      <div style="text-align: right">
                        <i class="fa fa-plus text-muted btn btn-simple round" style="padding: 10px; cursor:pointer; margin-right: 5px" title="Add field" ng-click="addDrug()"></i>
                          <i class="fa fa-minus text-muted btn btn-simple round" style="padding: 10px; cursor:pointer; margin-right: 5px" title="Remove last field" ng-click="removeDrug()"></i>
                      </div>
                    <div> 
                      <button class="btn btn-info" ng-click="toPatient()"   data-dismiss="modal">send to patient</button>
                      <button class="btn btn-danger" ng-click="toPharmacy()" aria-hidden="true">send to a pharmacy</button>
                    </div>

                  </div>

                  <div ng-if="isFindPharmacy" style="max-height: 900px;overflow-y: auto; overflow-x:hidden;">
                  <div>
                      <i class="fa fa-arrow-left font-1_1" ng-click="back('find')"> Back</i>
                  </div>
                    
                    <!--<h3>Find Pharmacy</h3>-->
                   
                    <form class="row">
                      <div class="col-md-8 col-sm-8 col-lg-8 form-group">  
                        <!--<label>City:</label>-->                        
                        <input class="form-control" type="text" placeholder="City" ng-model = "pharmacy.city"/>
                       </div>
                      
                      <div class="col-md-2 col-sm-2 col-lg-2">  
                        <input class="btn btn-info" type="button" value="find" ng-click = "findPharmacy()"/>
                      </div>
                    </form>
                    <div>
                        Drugs: <span ng-repeat="i in drugList"> {{i.drug_name}}, </span>
                    </div>
                    <hr class="bg-default">
                    <div ng-if="pharmacyData.length">
                        <!--<label>center name: </label>-->
                        <input class="form-control"  style="width:70%" type="text" placeholder="filter by name or address" ng-model = "pharmacy.name"/> 
                    </div>
                    <div class= "row look-up-phamarcy" ng-repeat = "result in pharmacyData | filter: pharmacy.name || pharmacy.city | orderBy: 'name'" style="cursor:pointer;margin: 10px 5px;" ng-click="forwardPrescriptionTo(result.user_id)">                      
                      <div style="float:left">
                        <img ng-src="{{result.profile_pic_url}}" style="max-width: 72px; max-height: 72px" alt="profile pic"/>
                      </div>
                      <div style="float:left;padding-left: 15px">
                        <strong>{{result.name}}</strong>
                        <span style="display: block;margin-top: -5px">{{result.address}} {{result.city}}, {{result.country}}</span>
                        <span style="display: block;margin-top: -5px">{{result.phone}}</span>   
                      </div>                    
                    </div>
                    <div ng-if="loading" style="text-align: center;margin-top: 50px">
                        <i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:orange" ng-if="loading"></i>
                    </div>
                  </div>
                  <div ng-if="isCenter">                  
                            
                            <div class="row" ng-if="placeHolder">
                            <button class="btn btn-simple ml-1 mt-1" ng-click="back('center')">Back</button>
                            
                              <div class="col-sm-12 col-md-12 col-lg-12">
                                    <h4 class="mb-1">Forwarding Prescription</h3>
                              <div style="color: red">{{message}}</div> 
                              <hr> 
                                <h4>{{centerInfo.name}} <span class="text-muted" style="display:block; font-size: 12px">{{centerInfo.type}}</span></h4>
                                <div class="col-sm-12 col-md-3 col-lg-3 pull-right">
                                  <img ng-src="{{centerInfo.profile_pic_url}}" alt="profile pic"/>
                                </div>
                                <div class="col-sm-12 col-md-9 col-lg-9">     
                                  <div><strong>Address: </strong>{{centerInfo.address}}</div>
                                  <div><strong>City: </strong>{{centerInfo.city}}</div>
                                  <div><strong>Country: </strong>{{centerInfo.country}}</div>
                                </div>
                              </div> 
                              <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                  <p style="padding: 15px">Do you wish to contact <strong>{{centerInfo.name}}</strong> for inquires before sending prescription? <span style="cursor:pointer; color: green" ng-click="isEnquiry()"> &nbsp;YES</span></p>
                                  <div ng-if="isContactFirst">
                                    <span style="font-size: 24px; font-weight: 100">Contact Through: </span>
                                    <i class="fa fa-phone btn btn-simple text-success" style="font-size:1em"></i>
                                    <i class="fa fa-commenting-o btn btn-simple text-success" style="font-size:1em"></i>       
                                  </div>      
                                </div>    
                              </div>
                              
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                 <button class="btn btn-success btn-block" style="cursor:pointer;" ng-click="sendReferral(centerInfo.user_id,centerInfo.type)">I have confirmed, send prescription</button>
                                   </div>
                              
                            </div>
                            <div class="mt-2" ng-if="success">
                             
                                <h5>{{message.info}} <span>To</span> <span>{{centerInfo.name}}</span></h5>
                                <address>{{centerInfo.address}}, <span style="display:block">{{centerInfo.city}}, {{centerInfo.country}}<span></address>
                                <p> Ref No: {{message.ref}}</p>
                                <p><strong>Make note of the <em style="font-weight: normal">Address</em> and <em style="font-weight: normal">reference number</em> above for easy location and identification respectively.</strong></p>
                              
                            </div>
                    
                  </div>            
                </div>
              
            </div>
          </div>
        </div>
     </script>


