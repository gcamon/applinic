            <div class="modal-body" style="max-height: 800px;overflow-y: auto;">  
              <h5 style="text-align:center;text-decoration: underline;">{{docInfo.skill.skill}}</h5>
              <section>
                <p style="font-size: 14px">{{docInfo.skill.procedure_description}}</p>
                <div ng-repeat = "file in docInfo.skill.files">
                  <div style="text-decoration: underline;" ng-if="file.type === 'video/mp4'">
                     <div>Video</div>
                     <video  style="max-width: 100%;height:auto" controls
                        poster="https://archive.org/download/WebmVp8Vorbis/webmvp8.gif"  class="img-responsive">  
                        <source
                          ng-src="{{file.path}}"
                          type="video/mp4">                                              
                        Your browser doesn't support HTML5 video tag.
                      </video>
                  </div>
                  <div style="text-decoration: underline;" ng-if="file.type === 'image/jpg' || file.type === 'image/jpeg' || file.type === 'image/png'">
                    <div>Image</div>
                    <img src="{{file.path}}" class="img-responsive" alt="Sample image" style="height:auto;max-width:100%">
                  </div>
                </div>                
              </section>                                                 
                 
            </div>
            <div class="modal-footer">
              <!--<button type="button" ng-click="close('No')" class="btn btn-default" data-dismiss="modal">No</button>
              <button type="button" ng-click="close('Yes')" class="btn btn-primary" data-dismiss="modal">Yes</button>-->
            </div>
          </div>
        </div>
      </div>
   </script>
   <script type="text/ng-template" id="doctor-refer-to-me.html">
       <div class="modal fade">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
            <div class="row">
              <button type="button" class="close" ng-click="close('Yes')" data-dismiss="modal">&times;</button>
               <img ng-src="{{docInfo.profile_pic_url}}" style="max-width:3em; max-height:3em"/>
               <span style="display:inline-block;padding: 10px">{{docInfo.title}} {{docInfo.firstname}} {{docInfo.lastname}}  <span style="display: block;font-size: 0.7em">{{docInfo.specialty}}</span></span> 
                          
            </div>
            <div class="modal-body shadow" style="padding: 20px;margin-bottom: 20px">
                <div ng-if="result">
                    <p style="color:green;text-align: center;">{{result}}</p>
                </div>  
                <h5 ng-cloak><i class="fa fa-hand-o-right" aria-hidden="true"></i> Refer patient to {{doc.title}} {{doc.firstname}} {{doc.lastname}}</h5>
                <form class="form-group">
                    <div>
                        <label>What is your complain?</label>
                        <textarea class="form-control" ng-model="patient.history" style="height:  100px" required></textarea>
                    </div>

                    <div class="space" ng-if="checkLogIn.typeOfUser !== 'Patient'">
                        <label>Patient phone number</label>
                        <small style="display: block;"><strong>Tip: </strong><span>Phone number is the number patient used to register on Applinic.com</span></small>
                        <input type="number" name="number" class="form-control" ng-model="patient.phone" required>
                    </div>
                  
                    <div class="space">
                        <button ng-click="send()" class="btn btn-success" style="position: static">Refer patient</button>
                        <button ng-if="checkLogIn.typeOfUser === 'Patient'" class="btn btn-success" ng-click="send()" style="position: static">Refer yourself</button>
                    </div>
                </form>
                
             
            </div>
            
          </div>
        </div>
      </div>
   </script>

