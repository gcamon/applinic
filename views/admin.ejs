	<script type="text/ng-template" id='pay-user.html'>
	         <div class="modal fade">
	          <div class="modal-dialog">
	            <div class="modal-content">
	              <div class="modal-header">
	              </div>              
	              <div class="modal-body" > 
	                 <h3 style="text-align:center">cash out details</h3>
	                 <div>
	                 	<div>
		                 	<label>NAMES: </label>
		                 	<span>{{info.firstname}} {{info.lastname}}</span>
		                 	<span ng-if="!i.firstname">{{info.name}}</span>
	                 	</div>
	                 	<div>
		                 	<label>BANK: </label>
		                 	<span>{{info.bank}}</span>	                 	
	                 	</div>
	                 	<div>
		                 	<label>ACCOUNT NUMBER: </label>
		                 	<span>{{info.account_number}}</span>	                 	
	                 	</div>
	                 	<div>
		                 	<label>PHONE NUMBER: </label>
		                 	<span>+234 {{info.phone}}</span>	                 	
	                 	</div>
	                 	<div>
		                 	<label>AMOUNT TO CASH OUT: </label>
		                 	<span>{{info.amount}}</span>	                 	
	                 	</div>
	                 	<p>Request was made on {{info.date | date: 'mediumDate'}} by {{info.date | date: "mediumTime"}}</p>
	                 </div>
	              </div>
	              <div class="modal-footer">
	                <button type="button" ng-click="goBack()" class="btn btn-primary" data-dismiss="modal">Close</button>	                
	              </div>
	            </div>
	          </div>
	        </div>
	    </script>

	    <script type="text/ng-template" id='admin-view-new-user.html'>
	         <div class="modal" style="transition: opacity .15s linear;">
	          <div class="modal-dialog">
	            <div class="modal-content">
	              <div class="modal-header">
	              	<h5>{{newUserDetails.when}}</h5>
	              </div>              
	              <div class="modal-body" > 
	                <table>
	                	<tr>
	                		<thead>
	                			<th>Date/time</th>
	                			<th>Type</th>
	                			<th>City</th>
	                			<th>Name</th>
	                			<th>phone</th>
	                		</thead>
	                	</tr>
	                	<tr>
	                		<tbody ng-repeat="j in newUserDetails.data | orderBy: '-date'">
	                			<td><small><span am-time-ago="j.date"></span> <span style="display: block;">{{j.date | date: "shortTime"}}</span></small></td>
	                			<td>{{j.type}}</td>
	                			<td>{{j.city}}</td>
	                			<td><span  ng-if="!j.name">{{j.title}} {{j.firstname}}</span> <span ng-if="j.name">{{j.name}}</span></td>
	                			<td>{{j.phone}}</td>
	                		</tbody>
	                	</tr>
	                </table>
	              </div>
	              <div class="modal-footer">
	                <button type="button" ng-click="goBack()" class="btn btn-primary" data-dismiss="modal">Close</button>	                
	              </div>
	            </div>
	          </div>
	        </div>
	    </script>

	    <script type="text/ng-template" id='admin-view-courier.html'>
	         <div class="modal" style="transition: opacity .15s linear;">
	          <div class="modal-dialog">
	            <div class="modal-content">
	              <div class="modal-header">
	              	<h5></h5>
	              </div>              
	              <div class="modal-body" style="line-height: 32px"> 
	                <div>
	                <h6>User Details</h6>
	                <b>Order ID: {{courierDetails.request_id}}</b><br>
	                <p class="text-success" ng-if="courierDetails.is_paid"><i class="fa fa-check"></i> <b>Paid for items plus delivery</b></p>
	                </div>
	                <div>
	                	<b>From</b> : {{courierDetails.firstname}}<br>
	                	<b>Date of request</b> : {{courierDetails.date | date : 'fullDate'}} ( <span am-time-ago="courierDetails.date"></span> )<br>
	                	<b>Address</b> : {{courierDetails.address}}<br>
	                	<b>City</b> : {{courierDetails.city}}<br>
	                	<b>Phone1</b> : {{courierDetails.phone1}}<br>
	                	<b>Phone2</b> : {{courierDetails.phone2}}<br>
	                </div>
	                <hr>
	                <div>
	                	<h6>Delivery Details</h6>
	                	<b>Dispatch center</b> : {{courierDetails.center_name}}<br>
	                	<b>Address</b> : {{courierDetails.center_address}}<br>
	                	<b>Phone</b> : {{courierDetails.center_phone}}<br>
	                </div>
	                <table>
	                	<thead>
	                		<th>sn</th>
	                		<th>name</th>
	                		<th>dosage</th>
	                		<th>frequency</th>
	                		<th>duration</th>
	                	</thead>
	                	<tbody ng-repeat="j in courierDetails.prescription_body">
	                		<td>{{j.sn}}</td>
	                		<td>{{j.drug_name}}</td>
	                		<td>{{j.dosage}}</td>
	                		<td>{{j.frequency}}</td>
	                		<td>{{j.duration}}</td>
	                	</tbody>
	                </table>
	                <hr>
	                <div>
	                	<b>Total cost of items</b> : {{courierDetails.total_cost}}<br>
	                	<b>Delivery charge</b> : {{courierDetails.delivery_charge}}<br>
	                </div>
	                
	              </div>
	              <div class="modal-footer">
	                <button type="button" ng-click="goBack()" class="btn btn-primary" data-dismiss="modal">Close</button>	                
	              </div>
	            </div>
	          </div>
	        </div>
	    </script>

	    <script type="text/ng-template" id='admin-refer-another.html'>
	         <div class="modal" style="transition: opacity .15s linear;">
	          <div class="modal-dialog">
	            <div class="modal-content">
	              <div class="modal-header">
	              	<h5>Redirect Consultaton</h5>
	              </div>              
	              <div class="modal-body" > 
	             	<div>
		              	<b>Patient Name:</b> {{user.patient_name}}
		              	<b>City:</b> {{user.patient_city}}
	              	</div>
	              	<div>
	              		<form ng-submit="find()" class="row">	              		  
	              			<div class="col-md-5">
	              				<input type="text" class="form-control" placeholder="Enter city name"  uib-typeahead="city for city in cities | filter:$viewValue | limitTo:15" ng-model="req.city" style="border: 1px solid #ccc">
	              			</div>
	              			<div class="col-md-5">
	              				<input type="text" class="form-control" placeholder="Enter specialty"   uib-typeahead="specialty.specialty for specialty in specialties | filter:$viewValue | limitTo:15" ng-model="req.specialty" style="font-size: 14px!important;border: 1px solid #ccc">
	              			</div>
	              			<div class="col-md-2 pt-3">
	              				<input type="submit" value="Find" class="btn btn-info">
	              			</div>
	              		</form>
	              	</div>
	              	<div style="height: 20px"></div>
	              	<div class="p-1 row" ng-repeat="result in result">
	              		<div class="col-md-3">
	              			<img src="{{result.profile_pic_url}}" style="width: 300px;height: auto;">
	              		</div>
	              		<div class="col-md-7">
	              			<span><b>{{result.title}} {{result.lastname}} {{result.firstname}}</b></span><br>
	              			<span>{{result.specialty}}</span><br>
	              			<span>{{result.work_place}} | {{result.address}}, {{result.city}} {{result.country}}</span>
	              		</div>
	              		<div class="col-md-2 pt-2">
	              			<a href="" ng-click="refer(user,result)" class="btn-danger btn btn-sm" ng-if="!result.msg && !result.loading">Refer</a>
	              			<span>{{(result.loading) ? "sending..." : result.msg}}</span>
	              		</div>
	              		<hr>
	              	</div>
	              	<div style="text-align: center;padding-top: 20px" ng-if="loading">
	              		<span>Loading...</span>
	              	</div>
	              </div>
	              <div class="modal-footer">
	                <button type="button" ng-click="goBack()" class="btn btn-primary" data-dismiss="modal">Close</button>	                
	              </div>
	            </div>
	          </div>
	        </div>
	    </script>


	    <script type="text/ng-template" id='admin-view-complaint.html'>
	         <div class="modal" style="transition: opacity .15s linear;">
	          <div class="modal-dialog">
	            <div class="modal-content">
	              <div class="modal-header">
	              	<div><b>{{complaint.patient_name}}</b></div>
	              	<div><b>{{complaint.patient_phone}}</b></div>   
	              </div>              
	              <div class="modal-body" > 
	              	<div><b>Complaint</b></div>
	              	<div><b>{{complaint.date | date : "fullDate"}}</b> ( <span am-time-ago="complaint.date"></span> )</div> 	              	          	

	              	<div class="pt-2" ng-bind-html="complaint.message">
	              	</div>
	              </div>
	              <div class="modal-footer">
	                <button type="button" ng-click="goBack()" class="btn btn-primary" data-dismiss="modal">Close</button>	                
	              </div>
	            </div>
	          </div>
	        </div>
	    </script>

	

