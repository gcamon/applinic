						<li><a href="#/scroll">Pending delivery&nbsp;&nbsp;<small style="color: red" ng-if="scrollList.length > 0">{{scrollList.length}}</small></a></li>
						<li><a href="#/pwr">PWR&nbsp;&nbsp;<small style="color: red" ng-if="pwr.length > 0">{{pwr.length}}</small></a></li>
