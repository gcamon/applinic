  <script type="text/ng-template" id="covid-chat-body.html">       
    <div class="modal fade">
      <div class="modal-dialog">
        <div class="modal-content" style="width: 900px; margin-left: -205px;">
          <div class="modal-header">
            <a href="" class="fa fa-times" data-dismiss="modal"></a>
          </div>
          <div class="modal-body">

            <form style="width: 60%;margin: 0 auto; padding: 20px" ng-submit="testPhone()" ng-if="!isVerificationSent && !isVerified">
              <div>
                <label>Please enter phone number</label><br>
                <input type="text" name="" placeholder="Enter phone number" value="Continue" class="form-control" ng-model="covidUser.phone" required>
              </div>
             
              <div class="text-center">
                <input type="submit" name="" class="btn btn-danger" value="Continue" ng-if="!loading">
                <i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color: orange" ng-show="loading"></i>
              </div>
            </form>
            <form style="width: 60%;margin: 0 auto;border: 1px solid rgba(0,0,0,0.2);padding: 20px" ng-submit="createAccount()" ng-if="isNewUser && isVerified">
                <p>
                  Please quickly fill the details below and continue.
                </p>
                <div>
                  <label>Title:</label><br>
                  <select class="form-control mt-0" ng-model="covidUser.patient_title" required>
                    <option>Mr.</option>
                    <option>Mrs.</option>
                    <option>Master</option>
                    <option>Miss</option>
                    <option>Dr.</option>
                    <option>Prof.</option>
                    <option>Engr.</option>
                  </select>
                </div>
                <div>
                  <label>First name:</label><br>
                  <input type="text" name="" class="form-control mt-0" ng-model="covidUser.patient_firstname" required>
                </div>

                 <div>
                  <label>Last name:</label><br>
                  <input type="text" name="" class="form-control mt-0" ng-model="covidUser.patient_lastname" required>
                </div>
                 <div>
                  <label>Date of Birth:</label><br>
                  <input type="date" name="" class="form-control mt-0" ng-model="covidUser.dob" required>
                </div>

                <div>
                  <label>Gender:</label><br>                  
                  <select class="form-control mt-0" ng-model="covidUser.patient_gender" required>
                    <option value="" disabled selected>_select</option>
                    <option>Male</option>
                    <option>Female</option>
                  </select>
                </div>

                <!--<div>
                  <label>Email:</label><br>
                  <input type="text" name="" class="form-control mt-0" ng-model="covidUser.email">
                </div>

                <div>
                  <label>Password:</label><br>
                  <input type="password" name="" class="form-control mt-0" ng-model="covidUser.password" required>
                </div>

                <div>
                  <label>Confirm Password:</label><br>
                  <input type="password" name="" class="form-control mt-0" ng-model="covidUser.password2" required>
                </div>-->
                <br>
                <div>
                  <button class="btn btn-primary">Continue</button>
                </div>

              </form>

              <form style="width: 60%;margin: 0 auto;border: 1px solid rgba(0,0,0,0.2);padding: 20px" ng-submit="verifyPhoneActive()" ng-if="isVerifyPin">
                <p>Please enter PIN sent to this "" to verify your phone number is active and move on to Chat with covid-19 doctors. </p>
                <div class="text-center">
                  <input type="text" name="" placeholder="Enter pin" class="form-control" ng-model="covidUser.pin"><br>
                  <input type="submit" name="" value="Verify and continue to Chat" class="btn btn-info">
                </div>
              </form>

            <!-- chat body -->
            <div class="messaging" ng-if="isVerified && !isNewUser">
              <div class="inbox_msg">
                <div class="inbox_people">
                  <div class="headind_srch">
                    <div class="recent_heading">
                      <label>Doctors</label>
                    </div>
                    <div class="srch_bar">
                      <!--<div class="stylish-input-group">
                        <input type="text" class="search-bar"  placeholder="Search" ng-model="ft">
                       </div>--> 
                    </div>
                  </div>
                  <div class="inbox_chat">
                    <div class="chat_list " ng-class="{'text-success' : !chat.is_read,'active_chat': chat.partnerId == partner.partnerId}" ng-repeat="chat in chatsList | orderBy : '-realTime' | filter: ft" ng-click="viewChat(chat)">
                      <div class="chat_people" >
                     
                        <div class="chat_img"> <img src="{{chat.profilePic}}" alt="image"> </div>
                        <div class="chat_ib">
                          <h5>{{chat.name}} <span class="chat_date" >{{ (chat.messages[chat.messages.length - 1].time  || chat.messages[chat.messages.length - 1].received ) | amCalendar}}</span></h5>
                          <p class="ellipsis">{{(!chat.typing) ? (chat.messages[chat.messages.length - 1].received || chat.messages[chat.messages.length - 1].sent ) : chat.typing }}</p>
                        </div>
                      </div>
                    </div>
                
                
                  </div>
                </div>
                <div class="mesgs" ng-if="isChat || isChat === undefined" style="position: relative;">
                  <div style="position: absolute;left: 25px;top: 5px">
                     <span class="ellipsis" ng-cloak><b>{{partner.name}}<!--{{partner.partnerType}}--> </b>&nbsp;
                        <span>
                          <em ng-if="partner.typing" style="font-size: 12px;color: green"><small>{{partner.typing}}</small></em> 
                          <em ng-if="partner.status && !partner.typing" style="font-size: 12px;color: green" ng-cloak>Online</em>
                          <em ng-if="!partner.status && !partner.typing" ng-cloak style="color:orange;font-size: 12px"><!--{{lastSeen(partner)}}--> Offline</em>
                          </span>
                      </span>
                    </div>
                    <div ng-if="checkLogIn.typeOfUser === 'Patient' || checkLogIn.typeOfUser === 'Doctor'" style="position: absolute;right: 25px;top: 5px"> 
                      
                      <label for="file-input">
                  <i class="fa fa-paperclip" aria-hidden="true" style="font-size: 18px;cursor: pointer;" title="Attach a file"></i>
                </label>&nbsp;&nbsp;&nbsp; <input type="file" class="file-input hide" name="file-input" id="file-input" file-model-chat="files">&nbsp;&nbsp;

                <a href="" title="Make video call">
                        <i class="fa fa-video-camera"></i>
                      </a> &nbsp;&nbsp;&nbsp;           
                  
                    <!--<a href="" class="ellipsis" ng-if="partner.partnerType === 'Doctor' && checkLogIn.typeOfUser !== 'Doctor'"  ng-click="chatPrivate(partner)">Single Chat</a>-->
                    <a href="" class="text-success text-danger " style="font-size: 14px;" ng-if="partner.partnerType === 'Patient'"  ng-click="chatPrivate(partner)" title="Go to e-treatment to generate bill for treatment or consultation, refer to a spacialist, write prescription, request diagnostic investigation or view medical records, etc." >Manage Patient</a>
                    
                  </div>

                    <div class="msg_history msg_container_base" id="base">

                    <div id="sentmessage">
                      
                    </div>
                    <div style="position: absolute;top:170px;right:320px" ng-if="loading">
                      <i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:#fff;font-size: 32px" ></i>
                    </div>


                 
                  </div>
                  <form class="type_msg" ng-submit="sendChat1()">
                    <div class="input_msg_write">
                      <input type="text" class="write_msg" placeholder="Type a message" ng-model="user.text1" />
                      <button type="submit" class="msg_send_btn"><i class="fa fa-paper-plane-o" aria-hidden="true"></i></button>
                      <!--<button type="submit" class="msg_send_btn"><i class="fa fa-paperclip" aria-hidden="true"></i></button>-->
                    </div>
                  </form>
                </div>
              </div>      
            </div>
          </div>
         
        </div>
      </div>
    </div>
  </script>



