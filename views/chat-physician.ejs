     <!-- quick chat modal -->
   <script type="text/ng-template" id='chat-auth.html'>
       <div class="modal fade">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header" style="position: relative;">   
                <h5><b>Tell us who you are!</b></h5>   
                <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true" style="color:red;font-size: 32px;z-index: 3;position: absolute;top: 8px;right: 15px">&times;</button>                         
            </div>
                    
            <div class="modal-body">
                <form ng-submit="createAccount('Patient')" ng-show="!isPhoneverify"> 
                    <p>Please quickly fill this form  so that the physician can know you.</p>
                    <div class="pt-3">
                        <label><b>Title</b></label><br>
                        <select placeholder="title" class="form-control" ng-model="user.title" required>
                            <option value="" disabled selected>_select title</option>                               
                            <option>Dr</option>
                            <option>Mr</option>
                            <option>Mrs</option>
                            <option>Miss</option>
                            <option>Master</option>
                            <option>Chief</option>
                            <option>Rev</option>
                            <option>Pastor</option>
                            <option>Engr</option>
                            <option>Barr</option>
                            <option>Surv</option>
                            <option>Hon</option>
                            <option>Ven</option>
                            <option>HRH</option>
                            <option>Prof</option>
                            <option>Justice</option>
                        </select>
                    </div>
                    <div class="pt-3">
                        <label><b>Firstname</b></label><br>
                        <input type="text" name="" placeholder="firstname" class="form-control" ng-model="user.firstname" required>
                    </div>

                    <div class="pt-3">
                        <label><b>Lastname</b></label><br>
                        <input type="text" name="" placeholder="lastname" class="form-control" ng-model="user.lastname" required>
                    </div>

                    <div class="pt-3">
                        <label><b>Date of Birth</b></label><br>
                        <input type="date" name="" placeholder="date of birth" class="form-control" ng-model="user.dob" required>
                    </div>

                    <div class="pt-3">
                        <label><b>Sex</b></label><br>
                        <!--<input type="text" name="" placeholder="sex" class="form-control" ng-model="person.gender" required>-->
                        <select placeholder="sex" class="form-control" ng-model="user.gender" required>
                            <option value="" selected disabled>_select</option>
                            <option>Male</option>
                            <option>Female</option>
                        </select>
                    </div>

                     <div class="pt-3" style="position: relative;">
                        <label><b>City</b></label><br>
                         <input type="text" name="" placeholder="City" class="form-control" ng-model="user.city" required uib-typeahead="city for city in cities | filter:$viewValue | limitTo:15">
                    </div>
                                       
                    <div class="pt-3">
                        <label><b>Phone Number</b></label><br>
                        <span class="text-danger">{{phoneMessage}}</span>
                        <input type="text"  placeholder="Enter phone number" class="form-control" ng-model="user.phone" required>
                    </div>
                    
                   
                    <div class="pt-3" style="position: relative;">
                        <label><b>Password</b></label><br>
                        <span class="text-danger">{{passwordMessage}}</span>
                        <input type="password" name="" placeholder="password" class="form-control" ng-model="user.password" id="myPass2" required>
                        <i class="fa fa-eye text-muted" style="font-size:20px;position: absolute;bottom: 10px;right:20px;color: grey;cursor: pointer;" onclick="showPass('b')"></i>
                    </div>

                    <div class="pt-3">
                        <label class="custom-control custom-checkbox ml-0 pl-0">
                          <input type="checkbox"  ng-model="user.agree" required>
                          <span class="custom-control-description" style="font-size: 14px;"> I agree with the <a href="/terms" target="_blank" >Terms of Service</a> and <a href="/privacy" target="_blank"> Privacy Policy.</a></span><br>
                          <span class="text-danger"></span> 
                        </label>
                    </div>
                    <div class="pt-3">
                        <button type="submit" name="" class="btn btn-danger btn-block"> <i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:#fff" ng-if="loading"></i> {{(loading) ? "" : "Continue"}}</button>
                    </div>
                    <p class="pt-2">Have an account? <a href="" class="text-danger" data-dismiss="modal" ng-click="signin()" >Log in</a></p>
                </form>   
                <form class="p-2"  ng-show="isPhoneverify">
                    <div  ng-if="!success" style="font-size: 14px">
                        <p class="text-success">{{verifyInfo}}</p>

                        <p class="text-danger p-2" style="border: 1px dashed #ccc">{{error}}</p>
                    </div>                    
                    <div ng-if="!success">
                        <p style="font-size: 14px">
                            If you did not receive SMS, get the verification pin via phone call. &nbsp;&nbsp;
                            <a href="" ng-click="call(resend)"><i class="fa fa-phone"></i> Call Now!</a>
                        </p> 
                        <p class="text-center" style="font-size: 14px" ng-if="showCallingMsg">{{showCallingMsg}}</p>
                    </div>
                   
                    <div ng-if="!success" class="col-md-11">
                        <input type="text" placeholder="Enter verification pin" ng-model="verify.pin" class="form-control">
                    </div>
                    <div class="clearfix"></div>
                    <div class="text-center">
                        <button class="btn btn-danger" ng-click="sendForm()" ng-if="!loading && !success">
                        <span class="sr-only">Loading...</span> Verify</button> 
                        <span ng-if="loading"><i class="fa fa-spinner fa-pulse fa-1x fa-fw text-info"  ng-if="loading"></i> verifying...</span>
                    </div>
                    <div ng-show="success" class="mt-3" style="text-align: center;">
                        <i class="fa fa-check text-success"></i>&nbsp;&nbsp;&nbsp;
                        <span class="text-muted">{{success}}</span><br>
                        <p class="p-2">Close modal and choose any physician you wish to chat with.</p>
                        <!--<a href="/login" class="btn btn-success btn-lg">Log In</a>-->
                    </div>
                </form>            
            </div>
            <div class="modal-footer">
            
            </div>
            
          </div>
        </div>
      </div>
    </script>

