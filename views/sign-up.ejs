     body {
        font-family: 'Open Sans', sans-serif;
     }
    input {color:#333 !important;
    font-size:.7em !important;}

    .noBorder{
      border:0px;
      padding: 0;
      margin: 0;
    }
        .card-title {
            text-transform: uppercase;
            text-align: center;
            border-bottom: 1px solid #ccc;
            padding-bottom: 1em;
        }
        .list-group-item{
            background: transparent;
            border:0;
        }
        .list-group-item:first-child, .list-group-item:last-child {
            background: transparent;
            border:0;
        }

        .form-control::-webkit-input-placeholder { color: rgba(0,0,0,0.5); font-size: 16px}  /* WebKit, Blink, Edge */
		.form-control:-moz-placeholder { color: rgba(0,0,0,0.5);font-size: 16px }  /* Mozilla Firefox 4 to 18 */
		.form-control::-moz-placeholder { color:rgba(0,0,0,0.5);font-size: 16px }  /* Mozilla Firefox 19+ */
		.form-control:-ms-input-placeholder { color: #000; font-size: 16px}  /* Internet Explorer 10-11 */
		.form-control::-ms-input-placeholder { color: #000; font-size: 16px}  /* Microsoft Edge */
		select {
			color: rgba(0,0,0,0.5);
			font-size: 14px
		}

		strong {
			font-size: 18px!important
		}

		.btn {
			background-color: #109cff;
			color: #fff!important
		}

        .inst {
            display: none;
        }

    @media screen and (max-width: 699px) {
        .inst {
            display: block;
        }

        .tab-pane {
            width: 100%!important
        }
    }
