<body ng-app = "myApp" style="overflow-x:hidden;">
    <!-- Navbar -->
    <% include ./inner/nav1 %>
    <!-- End Navbar -->

    <div class="clearfix"></div>

<div class="wrapper" style="height:60px;"></div>

<section class="container-fluid" ng-controller = "signupController">
   <div class="col-md-12 col-sm-12 col-xs-12 bg-info" style="padding:0; margin:1em 0;">
       <div class="row">
       <div class="col-md-4 col-sm-12 col-xs-12" style=" background:url('assets/images/bg3.jpg'); background-size:cover;">
       </div>

        <div class="col-md-8 col-sm-12 col-xs-12 bg-white p-1" style="padding:2em;" >
       <!-- Nav tabs -->
       <div class="pl-3">
        <h6 class="ml-2 pl-3 pt-3">Please select who you are?</h6>
        <span class="inst ml-2 pl-3">See form below.</span>
        <ul class="nav nav-tabs ml-2" role="tablist">
          <li class="nav-item">
            <a class="nav-link"  href="" ng-click="userType('Patient','patient-signup')" ng-class="{'active': user.typeOfUser === 'Patient'}"><b class="text-danger">Patient</b></a>
          </li>
          <li class="nav-item">
            <a class="nav-link"  href="" ng-click="userType('Doctor','doctor-signup')" ng-class="{'active': user.typeOfUser === 'Doctor'}"><b class="text-danger">Doctor</b></a>
          </li> 
           <li class="nav-item">
            <a class="nav-link" ng-click="userType('Special Center','special-center-signup')"  href="" ng-class="{'active': user.typeOfUser === 'Special Center'}"><b class="text-danger">Special Center</b></a>
          </li>
          <li class="nav-item">
            <a class="nav-link" ng-click="userType('Pharmacy','pharmacy-signup')"  href="" ng-class="{'active': user.typeOfUser === 'Pharmacy'}"><b class="text-danger">Pharmaceutical Center</b></a>
          </li>
           <li class="nav-item">
            <a class="nav-link" ng-click="userType('Diagnostic','diagnostic-signup')" href="" ng-class="{'active': user.typeOfUser === 'Diagnostic'}"><b class="text-danger">Diagnostic center</b></a>
          </li>
          
        </ul>
        </div>

    <!-- Tab panes -->
    <div class="container">
      <div class="tab-content p-1" style="min-height:540px;">
         <div ng-view class="p-0">
             
         </div>
      </div>
    </div>
	

       </div>
       </div>
   </div>

</section>







<div class="clearfix"></div>
