    <script type="text/ng-template" id='view-center-profile.html'>
         
        <div  class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
             <div class="modal-header">           
                <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true">&times;</button>
              </div>              
              <div class="modal-body container-fluid" style="margin-top: 10px"> 
                 <div class="row">
                    <div class="col-sm-12 col-md-3 col-lg-3 pull-right">
                        <img src="{{center.profile_pic_url}}" style="max-width: 120px; max-height: 120px">
                    </div>
                     <div class="col-sm-12 col-md-9 col-lg-9">
                      <div style="margin-left: 20px;border-bottom:1px solid rgba(0,0,0,0.2)"  class="star-rating">Center information</div>
                        <ul style="line-height: 35px;list-style: none;">
                            <li><strong>Center name : </strong> {{center.name}}</li>
                            <li><strong>Address : </strong> {{center.address}}</li>
                            <li><strong>City : </strong> {{center.city}}</li>
                            <li><strong>country : </strong> {{center.country}}</li>
                            <li><strong>Office hours : </strong> 
                                <ul>
                                     <li ng-repeat="office in center.office_hour"  style="list-style-type: none;font-size: 16px"> 
                                    <strong>{{office.day}}</strong> &nbsp;<span>{{office.from | date: "shortTime"}} - {{office.to | date: "shortTime"}}</span>
                                    </li>
                                </ul>
                            </li>
                            <li><strong>Office line : </strong> +234 {{center.phone}}</li>

                        </ul>

                        <div style="margin-left: 20px;margin-top: 10px"><div class="star-rating">
                            <span  ng-cloak > <span>Rating  </span>
                            <div class="star-rating"  star-rating rating-value="center.rating.current" max="center.rating.max"></div>
                            </span>
                            </div>
                        </div>
                    </div>
                 </div>
              </div>              
            </div>
          </div>
        </div>
    </script>

    <script type="text/ng-template" id='patient-view-scan-image.html'>         
        <div  class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
             <div class="modal-header">           
                <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true">&times;</button>
              </div>              
              <div class="modal-body container-fluid" style="margin-top: 10px"> 
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <img src="{{test.profile_pic_url}}" style="max-width: 120px; max-height: 120px;float: left;">
                        <article style="display: inline-block;padding: 10px">
                            <strong>{{test.center_name}}</strong>
                            <span style="display: block;">{{test.center_address}} {{test.center_city}} {{test.center_country}}</span>
                        </article>
                    </div>            
                </div>
                <div class="row">
                    <div class="col-sm-12 col-md-12 col-lg-12" ng-repeat = "file in test.files" style="padding: 20px 10px">
                        <img src="{{file}}" class="img-responsive">
                    </div>
                </div>
              </div>              
            </div>
          </div>
        </div>
    </script>

