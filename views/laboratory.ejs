                    <span style="display: block;">{{(person.laboratory)? person.laboratory.patient_phone : person.radiology.patient_phone }}</span>
                      <span style="display: block;">{{person.date | date: 'mediumDate' }}</span>
                  </div>
                </div>           
                <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true">&times;</button>
              </div>              
              <div class="modal-body"> 
              <hr>
                <div class="p-1">
                  <label>Clinical Summary: </label>
                  <p style="text-indent: 15px;display: inline-block;">{{(person.laboratory) ? person.laboratory.clinical_summary : person.radiology.clinical_summary}}</p>
                </div>
                <div class="p-1">
                  <label>Indications: </label>
                  <p style="text-indent: 15px;display: inline-block;">{{(person.laboratory) ? person.laboratory.indication : person.radiology.indication}}</p>
                </div>
                <div class="p-1" ng-if="person.laboratory.parity || person.radiology.parity">
                  <label>Parity: </label>
                  <p style="text-indent: 15px;display: inline-block;">{{(person.laboratory) ? person.laboratory.parity : person.radiology.parity}}</p>
                </div>
                <div class="p-1"  ng-if="person.laboratory.lmp || person.radiology.lmp">
                  <label>LMP: </label>
                  <p style="text-indent: 15px;display: inline-block;">{{(person.laboratory) ? person.laboratory.lmp : person.radiology.lmp}}</p>
                </div>
                <div class="p-1">
                  <label>Test Ran: </label>
                    <ol>
                      <li ng-if="person.laboratory" ng-repeat="test in person.laboratory.test_ran">
                        <span>{{test.name}}</span>
                      </li>
                       <li ng-if="person.radiology" ng-repeat="test in person.radiology.test_ran">
                        <span>{{test.name}}</span>
                      </li>
                    </ol>
                </div>
                <div class="p-1">
                  <label>Test Reports: {{(person.laboratory) ? getReport(person.laboratory.report) : getReport(person.radiology.report)}}</label>
                  <div ng-repeat="rep in reportList">
                    <label class="text-primary">{{rep.investigation}}</label>
                    <p style="text-indent: 15px">{{rep.report}}</p>
                  </div>
                </div>
                <div class="p-1">
                  <label class="text-primary">Conclusion:</label>
                  <p style="text-indent: 15px">{{(person.laboratory) ? person.laboratory.conclusion : person.radiology.conclusion}}</p>
                </div>
                <div class="p-1">
                  <label>Test Report date: {{person.service_date | date: 'mediumDate'}} 
                  ( <span am-time-ago="person.service_date"></span> )</label>                 
                </div>
                <!--<div style="text-align: right;" class="p-1">
                  <button class="btn btn-primary" ng-click="editReport(person)" class="close" data-dismiss="modal"><i class="fa fa-pencil"></i> edit report</button>
                </div>-->
              </div>              
            </div>
          </div>
        </div>
     </script>


   



