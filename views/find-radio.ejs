                 <div ng-if='typeOfSearch === "diagnostic"' class="mb-2">
                   <label><b>LMP ( female )</b></label><br>
                   <textarea class="form-control p-1" placeholder="Write..." ng-model="data.lmp" ng-keyup="autoExpand($event)" id="textarea"></textarea>
                 </div>
                 <div ng-if='typeOfSearch === "diagnostic"' class="mb-2">
                   <label><b>Parity ( female )</b></label><br>
                    <textarea class="form-control p-1" placeholder="Write..." ng-model="data.parity" ng-keyup="autoExpand($event)" id="textarea"></textarea>
                 </div>

                

                <div class="pt-3 pb-3">                   
                    
                    <input type="submit"  class="btn btn-success btn-block" value="Send" ng-if="!loading">
                    <i class="fa fa-spinner fa-pulse fa-1x fa-fw text-warning" style="font-size: 18px" ng-if="loading"></i>&nbsp;
                    
                     <!--<input type="button" value="Cancel" ng-click="cancel()" class="btn btn-outline-danger btn-block" ng-if="checkLogIn.typeOfUser === 'Patient' && !loading">-->
                    <!--<input type="button" value="New Person Form" ng-click="emp()" class="btn btn-simple" ng-if="isEMP">-->
                </div>            
	        </form>
	    </section>
		</section>
		<div ng-if="isSent" class="p-1">
	    <h5 class=" text-success"  style="text-align:center"> Sent successfully!!!</h5>
	    <div class="m-0"><strong class="font-1-1"> Center Name: </strong><em>{{data.name}}</em> </div> 
	    <div class="m-0"><strong class="font-1-1"> Center Location: </strong><em>{{data.address}}, {{data.city}}, {{data.country}}</em> </div> 
	    <div class="mb-1"><span class="font-1-2" style="color:red"> Ref No:  </span><em>{{result}}</em> </div> 
	    <div class="font-1-1">Please take note of the center name and 
	        reference number above. You will use the reference number to find this prescription at the center.
	    </div>
		</div>

		<div ng-show="isReferReq">
			<p>
				We observed that the patient you compiled this test(s) is not yet registered in the platform. Please invite the patient to register so that you can continue sending test(s).<br><br>
				<a href="" class="btn btn-info btn-sm" ng-click="invite()" ng-show="!inviteStatus">{{(loading) ? "Sending..." : 'Invite Now!'}}</a><br>
				<p class="text-success">{{invitationMsg}}</p>
			</p>
		</div>
  
              </div>
              
            </div>
          </div>
        </div>
    </script>


  

     <!-- quick chat modal -->
   <script type="text/ng-template" id='quick-chat.html'>
       <div class="modal fade">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="p-2">
            <div>
                <b class="font-1_3"><i class="fa fa-comments"></i> Messaging</b>
                <button type="button" class="close" ng-click="close('Yes')" data-dismiss="modal">&times;</button>      
            </div>
            <hr>
           
            <div class="modal-body">
                <header>
                   <b>{{center.name}}</b>
                   <span style="display: block;">{{center.address}} {{center.city}} {{center.country}}</span>
                </header>
              
                <div class="pt-3">
                    <textarea class="form-control p-1" ng-model="messageBody" placeholder="write..." style="min-height: 140px" ng-keyup="autoExpand($event)">
                    </textarea>
                </div>

                <div class="pt-2">
                    <button ng-click="sendChatSingle(center.id,center.user_id)" class="btn btn-danger btn-block" style="position: static;margin-left: 2px"><i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:#fff" ng-if="loading"></i> {{(isSent) ? "Message sent!" : 'Send'}}</button>
                </div>
               
            </div>
            <div class="modal-footer">
               <p class="p-2" ng-show="isSent">This is one-way messaging. If you want to view replies or continue chatting with this center, please go to your dashboard and click <i class="fa fa-comments"></i> icon to use our chat interface for two-way messaging. <a href="" ng-click="todashboard(checkLogIn.type)"><i class="fa fa-arrow-left"></i> Go to dashboard now!</a></p>
            </div>
            
          </div>
        </div>
      </div>
   </script>

