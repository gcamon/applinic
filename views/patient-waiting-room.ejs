            <main id="main" class="tg-haslayout" ng-controller="patientWaitingRoomController" style="margin-bottom:30px" id="main" ng-cloak>
                <section class="container">
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-lg-12">                           
                            <div class="waiting-roo">
                                <div class="complain-list" id="complaint">
                                    <!--<ul>
                                        <li><a href="#/1" ng-class="{'btn-primary':batch==='1'}" ng-click="getBatch('1')">1</a></li>
                                        <li><a href="#/2" ng-class="{'btn-primary':batch==='2'}" ng-click="getBatch('2')">2</a></li>
