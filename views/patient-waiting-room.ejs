                                    <section class="patient-complai  mt-3"> 
                                         <input type="text" ng-model="complain.text" placeholder="Filter by ID, City, Age, Gender, Symptoms or Disease ,  etc" class="form-control" style="margin-left: 0">                                       
                                        <div ng-if="i.symptoms[0] !== 'undefine'" ng-repeat="i in complaints | filter: complain.text | orderBy: '-sent_date'" class="card" style="color: rgba(0,0,0,0.8);font-size: 14px;display: block;background-color: #fff;box-shadow: 0 5px 25px 0 rgba(0,0,0,.2);">
                                            <div class="" style="border-right: 1px solid #ccc;width: 20%;display: inline-block;float: left;padding: 15px">
                                              <div>
                                                  <small style="color:green" ng-cloak>{{i.complaint_id}}</small>
                                              </div>
                                              <div>
                                                  <i class="fa fa-user" style="color: #000"></i> 
                                                &nbsp;<small style="color:green" ng-cloak>{{i.gender}}</small> 
                                              </div>
                                              <div>
                                               <small style="color:green" ng-cloak>{{i.age}}</small>
                                                
                                               </div> 
                                               <div>
                                                   <small class="fa fa-globe"> {{i.preferred_city || 'Anywhere'}}</small>
                                               </div>                                              
                                                
                                            </div>                            
                                            
                                            <div style="width: 80%;display: inline-block;padding: 15px;position: relative;">
                                                <header style="position: relative;">
                                                        <b style="color: #000;font-size: 12px;margin-top:20px;display: inline-block" ng-if="i.symptoms.length > 0">Symptoms | <span ng-repeat="symptom in i.symptoms" ng-cloak>{{symptom}} &nbsp;</span></b><br>
                                                        <time  am-time-ago="i.sent_date" style="position: absolute;right: 0;top:2px;font-size: 12px;font-weight: bold;"></time>
                                                </header>
                                                 <p class="pt-1" ng-bind-html = "i.description">
                                                        
                                                 </p>
                                                 <a href="" style="text-decoration:underlin;display: inline-block;position: absolute;right: 5px;bottom: 5px;padding:3px;font-size:normal;" ng-click="veiwDatail(i.complaint_id)" class="btn btn-info">Respond!</a>
                                                 <small  ng-if="i.files.length > 0" style="position: absolute;bottom: 5px;left: 10px;color: green">{{i.files.length}} <i class="fa fa-paperclip"> </i></small>
                                                
                                            </div>
                                           
                                            
                                        </div>

                                    </section>
                                    <div style="font-size: 22px;padding: 80px 100px" ng-if="loading">
                                       <i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:orange" ng-if="loading"></i> 
                                    </div>
                                    <ul class="pt-3 pagination">
                                        <li><a href="#/1" ng-class="{'btn-primary':batch==='1'}" ng-click="getBatch('1')">1</a></li>
                                        <li><a href="#/2" ng-class="{'btn-primary':batch==='2'}" ng-click="getBatch('2')">2</a></li>
                                        <li><a href="#/3" ng-class="{'btn-primary':batch==='3'}" ng-click="getBatch('3')">3</a></li>
                                        <li><a href="#/4" ng-class="{'btn-primary':batch==='4'}" ng-click="getBatch('4')">4</a></li>
                                        <li><a href="#/5" ng-class="{'btn-primary':batch==='5'}" ng-click="getBatch('5')">5</a></li>
                                        <li><a href="#/6" ng-class="{'btn-primary':batch==='6'}" ng-click="getBatch('6')">6</a></li>
                                        <li><a href="#/7" ng-class="{'btn-primary':batch==='7'}" ng-click="getBatch('7')">7</a></li>
                                        <li><a href="#/8" ng-class="{'btn-primary':batch==='8'}" ng-click="getBatch('8')">8</a></li>
                                        <li><a href="#/9" ng-class="{'btn-primary':batch==='9'}" ng-click="getBatch('9')">9</a></li>
                                        <li><a href="#/10" ng-class="{'btn-primary':batch==='10'}" ng-click="getBatch('10')">10</a></li>
                                    </ul>
                                </div>



                                <aside style="background-color: #fff;box-shadow: 0 10px 25px 0 rgba(0,0,0,.2); border-radius:5px 5px 0 0; top: 80px;" class="complain-in-view" ng-class="{'no-view':isViewPatient !== true}" id="complain-in-view">
                                    <div class="complaint-view-head" style="font-weight: bold;position: relative;">
                                        <i class="fa fa-user complaint-user" ng-cloak> &nbsp; Patient ({{patient.complaint_id}})</i>
                                        <i  style="position: absolute;bottom: 10px;right: 25px;padding: 0;" class="fa fa-times complaint-times"  ng-click="close()" ng-if="isViewPatient === true"></i>                                     
                                    </div>
                                    <section style="padding:10px;">                                        
                                        <header>
                                            <span><strong>Age</strong ng-cloak> | {{patient.age}}</span>&nbsp;&nbsp;
                                            <span><strong>Gender </strong ng-cloak> | {{patient.gender}}</span>&nbsp;&nbsp;
                                            <span><strong>Residence</strong ng-cloak> | {{patient.patient_city}} {{patient.patient_coutry}}</span>
                                        </header><hr>
                                        <aside>
                                           <div ng-if="patient.preferred_city" style=" background:#f2f2f2; text-transform:capitalize; font-size:16px;padding: 10px 0;text-align:center;margin: 15px 0" ng-cloak>Prefers doctors resident in {{patient.preferred_city}}</div>
                                        </aside>
                                        <h6 style="font-weight:bold">History</h6><hr/>
                                        <ul class="patient-symptoms">                                            
                                            <li ng-repeat="symptom in patient.symptoms" class="description"><strong ng-cloak>{{symptom}}</strong></li>
                                        </ul>
                                        <p class="pt-1" ng-bind-html= "patient.description"></p>
                                        <h6 style="font-weight:bold;" ng-if="patient.files.length > 0">Attached file(s)</h6><hr/>
                                        <div class="card" ng-if="patient.files.length > 0" ng-repeat="file in patient.files" >
                                            <div  ng-if="file.type === 'image'" style="width: 480px;height:auto;margin:5px auto">
                                            <img ng-src="{{file.url}}"  ng-click="viewFile()"  style="width: 100%;height:100%;margin-right:10px" class="img-responsive">
                                            </div>
                                            
                                            <video style="width:100%" controls
                                              poster="https://archive.org/download/WebmVp8Vorbis/webmvp8.gif" ng-if="file.type === 'video'"  class="img-responsive>  
                                              <source
                                                ng-src="{{file.url}}"
                                                type="video/mp4">                                              
                                              Your browser doesn't support HTML5 video tag.
                                            </video>
                                        </div>
                                    </section>
                                    <section class="pb-5" style="padding:10px">
                                        <!--<h6>{{patient.response.length}} doctor(s) has responded so far.</h6>-->
                                        <h6 style="font-weight:bold">Your Response</h6>
                                       
                                        <form class="complaint-view-charge">
                                            
                                            <div class="pt-2">
                                                <label style="font-weight:bold;font-size: 14px">Consultation Fee </label> 
                                                &nbsp;&nbsp;( <small>20% discount for consultation applied</small> )         
                                            </div>
                                            <div class="clearfix"></div>
                                            <div class="pb-2">
                                                <small>Your charge = </small>
                                                <input type="number" class="form-control input-lg" placeholder="Enter amount" ng-model="user.amount">      
                                            </div>
                                            <div class="mt-1 mb-2">
                                                <small>You'll receive : {{str}} </small>
                                            </div>
                                          
                                            <label style="font-weight:bold;font-size: 14px" class="text-capitalize">Why should you be consulted ? </label>
                                            <textarea class="form-control" style="padding: 10px" cols="6" ng-model="user.introductory"></textarea>
                                            <div class="p-2"></div>
                                            <div class="pb-5 mt-2" style="display: block;margin-top: 20px">
                                                <button  ng-click="sendRequest()" class="btn btn-primary text-white"><i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:#fff !important" ng-if="loading"></i> {{(loading) ? '' : 'Submit'}}</button>
                                            </div>
                                        </form>
                                    </section>
                                </aside>
                            </div>
                        </div>                                        
                    </div>
                </section>
            </main>
