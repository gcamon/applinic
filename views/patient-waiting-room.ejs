             <header id="header" class="container-fluid"  ng-controller="topHeaderController" style="background-color:#fff;position: fixed;width: 100%;z-index: 999;">
              
                <div class="container" style="position: relative;">
                    <nav class="row">
                        <div class="col-sm-6 col-md-2 col-lg-2 pb-2 pt-3">
                            
                            <!--<a href="" ng-href="{{back}}" class="nav-link btn btn-simple btn-sm" style="color:#555!important;float: left;">
                              <b> &larr; Back</b>
                            </a> &nbsp;-->

                              <a class="brand text-success" href="/" style="text-decoration: none;">
                                <div class="text-muted"> <span class="l l1"></span> <span class="l l2"></span> <span class="l l3"></span> <span class="l l4"></span> <span class="l l5"></span> </div> <b>Applinic</b> </a>  
                                                
                        </div>  
                         <div class="col-sm-12 col-md-8 col-lg-8 pb-2 pt-3">
                           <h5><b>Patients' Waiting Room</b> &nbsp;</h5>
                        </div>                                     
                    </nav>
                    <a class="btn btn-simple text-danger btn-sm"  href="" ng-click="logout()" style="position: absolute;top: 15px;right: 0">
                        <span><i class="fa fa-power-off icon"></i>
                            Logout
                        </span> 
                    </a>
                </div>
            </header>
            <!--************************************
                            Header End
            *************************************-->
            
            <!--************************************
                            Main Start
            *************************************-->
           
            <main id="main" class="tg-haslayout complaint-main" ng-controller="patientWaitingRoomController" style="" ng-cloak>
                <section class="container">                    
                    
                          <div ng-show='stateIsLoading' style="text-align: center;padding-top: 10px;">
                            <span class="btn btn-warning btn-sm">Loading...</span>
                          </div>  
                          <div ng-show='!onlineStatus' style="text-align: center;padding-top: 10px;" ng-cloak>
                            <span class="btn btn-warning btn-sm text-white font-1_2">No internet connections
                            </span>
                          </div>
                          <div ng-show='onlineStatus && !acknowledged && !stateIsLoading' style="text-align: center;padding-top: 10px;" ng-cloak>
                            <span class="btn btn-success btn-sm text-white font-1_2">You're connected to internet
                            </span>
                          </div>                                                
                          <div>
                                <section id="complaint" class="col-sm-12 col-md-8 col-lg-8"> 
                                    <div class="p-1">                               
                                      <input type="text" ng-model="complain.text" placeholder="Filter by ID, City, Age, Gender, Symptoms or Disease ,  etc" class="form-control" style="margin-left: -20px "> 
                                    </div>                                      
                                    <div ng-repeat="i in complaints | filter: complain.text | orderBy: '-sent_date'" class="card row" style="position: relative;">
                                        <div style="height: 20px">
                                          
                                        </div>
                                        <div class="p-2 pb-0">
                                          
                                          <span  class="text-muted" ng-cloak>{{i.complaint_id}}</span> |
                                          
                                          
                                           
                                          <span class="text-muted" style="color:green" ng-cloak><i class="fa fa-user" style="color: #000"></i> {{i.gender}}</span> |
                                          
                                          
                                          <span class="text-muted" ng-cloak>{{i.age}}</span> <br>
                                            
                                           
                                          
                                          <span class="text-muted" class="fa fa-globe">Prefers doctors resident in <b>{{(i.preferred_city && i.preferred_city !== 'undefined') ? i.preferred_city : 'any state'}}</b></span>                                      
                                         
                                        </div>                            
                                         <time  am-time-ago="i.sent_date" style="position: absolute;right: 10px;top:5px;font-size: 12px;font-weight: bold;"></time>
                                        <div class="p-2 pt-0">
                                            <div style="position: relative;">
                                                <b style="color: #000;font-size: 12px;margin-top:20px;display: inline-block" ng-if="i.symptoms.length > 0"><span class="text-muted">Symptoms :</span> <span ng-repeat="symptom in i.symptoms" ng-cloak>{{symptom}}, &nbsp;</span></b><br>
                                               
                                            </div>
                                             <p class="p-1" ng-bind-html = "i.description">
                                                    
                                             </p>
                                             <a href="" style="position: absolute;right: 5px;bottom: 5px;padding:3px;font-size:normal;" ng-click="veiwDatail(i.complaint_id)" class="btn btn-info btn-sm">Respond!</a>
                                             <small  ng-if="i.files.length > 0" style="position: absolute;bottom: 5px;left: 10px;color: green">{{i.files.length}} <i class="fa fa-paperclip"> </i></small>
                                            
                                        </div>
                                       
                                        
                                    </div>

                                    
                                    <div style="font-size: 22px;padding: 80px 100px" ng-if="loading">
                                       <i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:orange" ng-if="loading"></i> 
                                    </div>
                                    <ul class="pt-3 pagination">
                                        <li ng-if="complaints.length > 20" ><a href="#/1" ng-class="{'btn-primary':batch==='1'}" ng-click="getBatch('1')">1</a></li>
                                        <li ng-if="complaints.length > 20"><a href="#/2" ng-class="{'btn-primary':batch==='2'}" ng-click="getBatch('2')">2</a></li>
                                        <li ng-if="complaints.length > 40"><a href="#/3" ng-class="{'btn-primary':batch==='3'}" ng-click="getBatch('3')">3</a></li>
                                        <li ng-if="complaints.length > 60"><a href="#/4" ng-class="{'btn-primary':batch==='4'}" ng-click="getBatch('4')">4</a></li>
                                        <li ng-if="complaints.length > 80"><a href="#/5" ng-class="{'btn-primary':batch==='5'}" ng-click="getBatch('5')">5</a></li>
                                        <li ng-if="complaints.length > 100"><a href="#/6" ng-class="{'btn-primary':batch==='6'}" ng-click="getBatch('6')">6</a></li>
                                        <li ng-if="complaints.length > 120"><a href="#/7" ng-class="{'btn-primary':batch==='7'}" ng-click="getBatch('7')">7</a></li>
                                        <li ng-if="complaints.length > 140"><a href="#/8" ng-class="{'btn-primary':batch==='8'}" ng-click="getBatch('8')">8</a></li>
                                        <li ng-if="complaints.length > 160"><a href="#/9" ng-class="{'btn-primary':batch==='9'}" ng-click="getBatch('9')">9</a></li>
                                        <li ng-if="complaints.length > 180"><a href="#/10" ng-class="{'btn-primary':batch==='10'}" ng-click="getBatch('10')">more...</a></li>
                                    </ul>
                                </section>



                                <aside style="background-color: #fff;box-shadow: 0 10px 25px 0 rgba(0,0,0,.2); border-radius:5px 5px 0 0; top: 80px;" class="complain-in-view" ng-class="{'no-view':isViewPatient !== true}" id="complain-in-view">
                                    <div class="complaint-view-head" style="position: relative;">
                                        <i class="fa fa-user" ng-cloak> &nbsp; Patient ({{patient.complaint_id}})</i>
                                        <i  style="position: absolute;bottom: 10px;right: 25px;padding: 0;" class="fa fa-times complaint-times"  ng-click="close()" ng-if="isViewPatient === true"></i>                                     
                                    </div>
                                    <section style="padding:10px;">                                        
                                        <header>
                                            <span><strong>Age</strong ng-cloak> | {{patient.age}}</span>&nbsp;&nbsp;
                                            <span><strong>Gender </strong ng-cloak> | {{patient.gender}}</span>&nbsp;&nbsp;
                                            <span><strong>Residence</strong ng-cloak> | {{patient.patient_city}} {{patient.patient_coutry}}</span>
                                        </header><hr>
                                        <aside>
                                           <div ng-if="patient.preferred_city" style=" background:#f2f2f2; text-transform:capitalize; font-size:16px;padding: 10px 0;text-align:center;margin: 15px 0" ng-cloak>Prefers doctors resident in {{(patient.preferred_city && patient.preferred_city !== 'undefined') ? patient.preferred_city : 'any state'}}</div>
                                        </aside>
                                        <h6 style="font-weight:bold">History</h6><hr/>
                                        <ul class="patient-symptoms">                                            
                                            <li ng-repeat="symptom in patient.symptoms" class="description"><strong ng-cloak>{{symptom}}</strong></li>
                                        </ul>
                                        <p class="pt-1" ng-bind-html= "patient.description"></p>
                                        <h6 style="font-weight:bold;" ng-if="patient.files.length > 0">Attached file(s)</h6><hr/>
                                        <div class="card" ng-if="patient.files.length > 0" ng-repeat="file in patient.files" >
                                            <div  ng-if="file.type === 'image'" style="width: auto;height:auto;margin:5px auto">
                                            <img src="{{file.url}}"  ng-click="viewFile()"  style="max-width: 480px;max-height:280px;margin-right:10px" class="img-responsive">
                                            </div>
                                            
                                            <video style="width:100%;height: auto;" controls
                                              poster="https://archive.org/download/WebmVp8Vorbis/webmvp8.gif" ng-if="file.type === 'video'"  class="img-responsive>  
                                              <source
                                                ng-src="{{file.url}}"
                                                type="video/mp4">                                              
                                              Your browser doesn't support HTML5 video tag.
                                            </video>
                                        </div>
                                    </section>
                                    <section class="pb-5" style="padding:10px">
                                        <!--<h6>{{patient.response.length}} doctor(s) has responded so far.</h6>-->
                                        <h6 style="font-weight:bold">Your Response</h6>
                                       
                                        <form class="complaint-view-charge">                                          
                                            
                                          
                                            <label style="font-weight:bold;font-size: 14px" class="text-capitalize">Management Plan? </label>
                                            <textarea class="form-control" ng-model="user.introductory" ng-keyup="autoExpand($event)" id="textarea"></textarea>
                                            <div class="p-2"></div>
                                            
                                            <div class="pt-2">
                                                <label style="font-weight:bold;font-size: 14px">Consultation Fee </label> 
                                                &nbsp;&nbsp;( <small>{{checkLogIn.city_grade}}% discount for consultation applied</small> )         
                                            </div>
                                            <div class="clearfix"></div>
                                            <div class="pb-2">
                                                <small>Your charge: </small>
                                                <input type="number" class="form-control input-lg" placeholder="Enter amount" ng-model="user.amount">      
                                            </div>
                                            <div class="mt-1 mb-2">
                                                <small>You'll receive : {{str}} </small>
                                            </div>

                                            <div class="mb-5 mt-2">
                                                <a href=""  ng-click="sendRequest()" class="btn btn-danger text-white"><i class="fa fa-spinner fa-pulse fa-1x fa-fw" style="color:#fff !important" ng-if="loading"></i> {{(loading) ? '' : 'Submit'}}</a>
                                            </div>
                                            <div style="height: 100px">
                                              
                                            </div>
                                        </form>
                                    </section>
                                </aside>
                            </div>

                </section>
            </main>
            <!--************************************
                            Main End
            *************************************-->
            <!--************************************
                            Footer Start
            *************************************-->
            <footer id="footer" class="tg-haslayout">
          
            <div class="tg-footerba tg-haslayout">
               <div class="copyright" style="color: #fff">
                    &copy;
                    <script>
                        document.write(new Date().getFullYear())
                    </script>, applinic.com
                </div>
            </div>
        </footer>
            <!--************************************
                            Footer End
            *************************************-->
        </div>
        <!--************************************
                        Wrapper End
        *************************************-->
