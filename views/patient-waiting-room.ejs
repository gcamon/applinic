                <nav class="header" style="height:70px; margin-top:-.5em; z-index:1000;">
                     <div class="header-block header-block-nav">
                        <ul  style="display:inline-block;width:100%">
                            <li style="float: left;margin:15px 10px;list-style: none;">
                                <a href="" ng-href="{{back}}" class="nav-link" style="">
                                  <b > &larr; Back</b>
                                </a>
                            </li>                  

                            <li style="float:left;margin:20px 10px;list-style: none;">
                              
                                 <a class="brand" href="/" style="margin-left:3em;display: inline-block;text-decoration: none;">
                                <div class="logo"> <span class="l l1"></span> <span class="l l2"></span> <span class="l l3"></span> <span class="l l4"></span> <span class="l l5"></span> </div> Applinic </a>                                
                            </li>
                             
                            <li style="float: left;margin:20px 10px;list-style: none;">
                                <h5><b>PWR</b></h5>
                            </li>  

                             <li style="float: left;margin:15px 10px;list-style: none; width: 300px">
                                <input ng-model="complain.text" placeholder="Filter by id city age gender or symptoms etc" class="form-control">
                            </li>  
                            <li style="float:right;list-style-type:none;margin:20px 10px">
                                <a class="nav-link" href="" ng-click="logout()">
                                   
