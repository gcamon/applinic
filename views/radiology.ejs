                            <label>Email Address</label>
                            <input type="text" class="form-control" placeholder="Email" ng-model="patient.email" required/>
                        </div>
                        
                        <div class="col-md-12 pl-1">
                            <input type="button"  value="Submit" class="btn btn-primary" ng-click="sendForm('radiology')"/>
                        </div>
                        </div>
