    <script type="text/ng-template" id="write-prescription-modal.html">
        <div class="">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
              <div class="title">Test Result </div>
                
                    <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true" style="color:red;font-size: 32px">&times;</button>
                </div>                                     
             
              
              <div class="modal-body">
                    <div ng-if="isToPrescribe">
                      
                      <ul class="nav">
                        <li ng-repeat="test in testObj.test_result"><strong>{{test.name}} :</strong> {{test.data}}</li>
                      </ul>
                      <p><strong>Conclusion :</strong> {{testObj.conclusion}}</p>
                      <div class="form-inline">
                        <label>Write provisional diagnosis</label>
                        <input type="text" class="form-control" placeholder="What is wrong with this patient?" ng-model="patient.provisional_diagnosis">
                      </div> 
                      <div>
                      {{drugList}}
                        <table class="table table-bordered table-responsive">
                          <tr>
                            <thead>
                              <th><strong>S/N</strong></th>
                              <th><strong>Drug name</strong></th>
                              <th><strong>Dosage</strong></th>
                              <th><strong>Frequency</strong></th>
                              <th><strong>Duration</strong></th>
                            </thead>
                          </tr>
                          <tr>
                            <tbody ng-repeat="prescription in drugList">                
                              <td>{{prescription.sn}}</td>
                              <td><input name="states" id="states" type="text" placeholder="Enter drug name" ng-model="drug.name" uib-typeahead="drug.name for drug in drugs | filter:$viewValue | limitTo:15" class="form-control" style="width: 95%">
                                {{getDrug(drug.name)}} </td>
                              <td><input type="text" class="form-control" ng-model="prescription.dosage"></td>
                              <td><input type="text" class="form-control" ng-model="prescription.frequency"></td>
                              <td><input type="text" class="form-control" ng-model="prescription.duration"></td>
                            </tbody>
                          </tr>           
                        </table>
                      </div>
                      <div style="text-align: right">
                        <i class="fa fa-plus text-muted" style="font-size:16px;padding: 10px; cursor:pointer; margin-right: 5px" title="Add field" ng-click="addDrug()"></i>
                          <i class="fa fa-minus text-muted" style="font-size:16px; padding: 10px; cursor:pointer; margin-right: 5px" title="Remove last field" ng-click="removeDrug()"></i>
                      </div>
                    <div> 
                      <button class="btn btn-info" ng-click="toPatient()">Forward prescription to patient</button>
                      <button class="btn btn-info" ng-click="toPharmacy()" data-dismiss="modal" aria-hidden="true">Forward prescription to a Pharmacy<button>
                    </div>

                  </div>

                  <div ng-if="isFindPharmacy" style="max-height: 900px;overflow-y: auto; overflow-x:hidden;">
                    <button class="btn btn-simple" ng-click="back('find')">Back</button>
                    <h3>Find Pharmacy</h3>
                    <p><strong>Tips: </strong> You can filter the result by "name"  <br>or complete the text box and click "find" to search based on the entered criteria.</p>
                    <form>
                        <div class="row">
                            <div class="col-md-6 form-group">
                                <label>Pharmacy City/Town</label>        
                      <input class="form-control" type="text" placeholder="City/Town" ng-model = "pharmacy.city"/>
                            </div><div class="col-md-6 form-group">
                                <label>Pharmacy Name</label>
                      <input class="form-control"  type="text" placeholder="Name" ng-model = "pharmacy.name"/> 
                      </div>  
                      <input class="btn btn-info" type="button" value="FIND" ng-click = "findPharmacy()"/>
                    </div>
                    </form>
                    <div class= "row look-up-phamarcy" ng-repeat = "result in pharmacyData | filter: pharmacy.name || pharmacy.city | orderBy: 'name'" style="cursor:pointer;" ng-click="forwardPrescriptionTo(result.user_id)"> 
                      <div class="row">
                      <div class="col-md-4 col-sm-12">
                        <img ng-src="{{result.profile_pic_url}}" src="assets/img/avatar.jpg" alt="profile pic"/>
                      </div>
                      <div class="col-md-8 col-sm-12 " >
                        <h3>{{result.name}}</h3>
                        <address>{{result.address}} <span>{{result.city}}, <strong>{{result.country}}</strong></span></address>     
                      </div>
                      </div>
                    </div>
                  </div>
                  <div ng-if="isCenter">                  
                            
                            <div class="row" ng-if="placeHolder">
                            <button class="btn btn-simple ml-1 mt-1" ng-click="back('center')">Back</button>
                            
                              <div class="col-sm-12 col-md-12 col-lg-12">
                                    <h4 class="mb-1">Forwarding Prescription</h3>
                              <div style="color: red">{{message}}</div> 
                              <hr> 
                                <h4>{{centerInfo.name}} <span class="text-muted" style="display:block; font-size: 12px">{{centerInfo.type}}</span></h4>
                                <div class="col-sm-12 col-md-3 col-lg-3 pull-right">
                                  <img ng-src="{{centerInfo.profile_pic_url}}" alt="profile pic"/>
                                </div>
                                <div class="col-sm-12 col-md-9 col-lg-9">     
                                  <div><strong>Address: </strong>{{centerInfo.address}}</div>
                                  <div><strong>City: </strong>{{centerInfo.city}}</div>
                                  <div><strong>Country: </strong>{{centerInfo.country}}</div>
                                </div>
                              </div> 
                              <div class="row">
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                  <p style="padding: 15px">Do you wish to contact <strong>{{centerInfo.name}}</strong> for inquires before sending prescription? <span style="cursor:pointer; color: green" ng-click="isEnquiry()"> &nbsp;YES</span></p>
                                  <div ng-if="isContactFirst">
                                    <span style="font-size: 24px; font-weight: 100">Contact Through: </span>
                                    <i class="fa fa-phone btn btn-simple text-success" style="font-size:1em"></i>
                                    <i class="fa fa-commenting-o btn btn-simple text-success" style="font-size:1em"></i>       
                                  </div>      
                                </div>    
                              </div>
                              
                                <div class="col-sm-12 col-md-12 col-lg-12">
                                 <button class="btn btn-success btn-block" style="cursor:pointer;" ng-click="sendReferral(centerInfo.user_id,centerInfo.type)">I have confirmed, send prescription</button>
                                   </div>
                              
                            </div>
                            <div class="mt-2" ng-if="success">
                             
                                <h5>{{message.info}} <span>To</span> <span>{{centerInfo.name}}</span></h5>
                                <address>{{centerInfo.address}}, <span style="display:block">{{centerInfo.city}}, {{centerInfo.country}}<span></address>
                                <p> Ref No: {{message.ref}}</p>
                                <p><strong>Make note of the <em style="font-weight: normal">Address</em> and <em style="font-weight: normal">reference number</em> above for easy location and identification respectively.</strong></p>
                              
                            </div>
                    
                  </div>            
                </div>
              
            </div>
          </div>
        </div>
     </script>


     <script type="text/ng-template" id="patient-emergency-form.html">
         <div class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">

